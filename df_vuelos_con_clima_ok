        "from skl2onnx import convert_sklearn\n",
        "from skl2onnx.common.data_types import FloatTensorType\n",
        "\n",
        "# 1. Definir la entrada: \"Una fila con X columnas decimales\"\n",
        "# X.shape[1] es la cantidad de variables (dummies) que tienes\n",
        "initial_type = [('float_input', FloatTensorType([None, X.shape[1]]))]\n",
        "\n",
        "# 2. Convertir el modelo rf_smote a ONNX\n",
        "onx = convert_sklearn(rf_smote, initial_types=initial_type)\n",
        "\n",
        "# 3. Guardar el archivo REAL para predecir\n",
        "with open(\"modelo_prediccion.onnx\", \"wb\") as f:\n",
        "    f.write(onx.SerializeToString())\n",
        "\n",
        "print(\"Descarga 'modelo_prediccion.onnx'. ¡Este SÍ sirve para Java!\")"
